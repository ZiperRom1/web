{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include"       : ["app", "lib"],
        "exclude"       : ["dist", "node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks"          : false,
        "monospaceLinks"       : false,
        "outputSourcePath"     : true,
        "footer"               : "",
        "copyright"            : "DocStrap Copyright Â© 2012-2014 The contributors to the JSDoc3 and DocStrap projects.",
        "protocol"             : "html://",
        "systemName"           : "WebSocket services / AMD modules",
        "navType"              : "inline",
        "theme"                : "journal",
        "linenums"             : true,
        "collapseSymbols"      : false,
        "inverseNav"           : true,
        "outputSourceFiles"    : true,
        "dateFormat"           : "YYYY-MM-DD H:mm:ss",
        "highlightTutorialCode": false
      },
    "opts": {
        "destination": "../../../web-doc/jsDoc/",
        "template": "../ink-docstrap/template"
    }
}
