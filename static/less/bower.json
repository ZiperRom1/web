{
  "name": "style",
  "description": "bootstrap style custom less configuration",
  "main": "style.css",
  "dependencies": {
    "bootstrap": "3.3.6",
    "bootstrap-select": "latest",
    "bootstrap-switch": "latest"
  },
  "overrides": {
    "bootstrap-switch": {
      "main": "src/less/bootstrap3/bootstrap-switch.less"
    },
    "bootstrap-select": {
      "main": "less/bootstrap-select.less"
    },
    "bootstrap": {
      "ignore": true
    }
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}
